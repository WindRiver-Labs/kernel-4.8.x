/dts-v1/;

#include "axc67xx.dtsi"
#include "axc6712-cpus.dtsi"

/ {
	model = "Lionfish Emulation";
	compatible = "lsi,axc6732";

	memory {
		device_type = "memory";
		reg = <0 0x00000000 0x40000000>;
	};

	soc {
	};
};

&serial0 {
	status = "okay";
};

&nemac {
	status = "okay";
	phy-handle = <&phy0>;
};


&trng {
	status = "okay";
};

&mtc {
	status = "okay";
};


&mdio0 {
	status = "okay";
	lsi,mdio-clk-offset = <0x5>;
	lsi,mdio-clk-period = <0xc>;

	phy0: ethernet-phy@3 {
		reg = <0x3>;
		max-speed = <10>;
	};
};
